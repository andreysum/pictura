<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta content='text/html' http-equiv='Content-Type'>
        <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no' name='viewport'>

        <title>{% block title %}Pictura{% endblock %}</title>

        {% block stylesheets %}
        {% endblock %}

        {% stylesheets '@AppBundle/Resources/public/genericons/genericons.css' output='bundles/app/styles/genericons.css' filter='cssrewrite'%}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media='all'/>
        {% endstylesheets %}
        {% stylesheets '@AppBundle/Resources/public/style.scss' output='bundles/app/styles/style.css' filter='cssrewrite'%}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media='all'/>
        {% endstylesheets %}

        {% stylesheets
        output='bundles/app/styles/fonts/basictitlefont.ttf'
        '@AppBundle/Resources/public/fonts/basictitlefont.ttf' filter='cssrewrite'
        %}{% endstylesheets %}
        {% stylesheets
        output='bundles/app/styles/fonts/swscrpc.ttf'
        '@AppBundle/Resources/public/fonts/swscrpc.ttf' filter='cssrewrite'
        %}{% endstylesheets %}
        <style>
            @font-face {
                font-family: swscrpc;
                src: url({{asset('bundles/app/fonts/swscrpc.ttf')}}) format('truetype');
            }

            @font-face {
                font-family: Basic Title Font;
                src: url({{asset('bundles/app/fonts/basictitlefont.ttf')}}) format('truetype');
            }
        </style>
{#      <meta content='website' property='og:type'>
        <meta content='Fukasawa' property='og:title'>
        <meta content='Just another WordPress site' property='og:description'>
        <meta content='http://www.andersnoren.se/themes/fukasawa/' property='og:url'>
        <meta content='Fukasawa' property='og:site_name'>
        <meta content='https://s0.wp.com/i/blank.jpg' property='og:image'>
        <meta content='en_US' property='og:locale'>
#}
    </head>
    <body class='home blog wp-is-not-mobile infinite-scroll'>
        {{ render(controller('AppBundle:Pictura:navigation')) }}
        {% javascripts '@AppBundle/Resources/public/js/jquery/jquery.js' output='bundles/app/js/jquery/jquery.js' filter='cssrewrite'%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/js/masonry/masonry.min.js' output='bundles/app/js/masonry/masonry.min.js' filter='cssrewrite'%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% javascripts '@AppBundle/Resources/public/js/global.js' output='bundles/app/js/global.js' filter='cssrewrite'%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}

    </body>
</html>
